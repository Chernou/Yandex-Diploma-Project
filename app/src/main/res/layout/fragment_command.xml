<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feature.command.presentation.fragments.CommandFragment">

    <TextView
        android:id="@+id/toolbar_settings"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dimens_16"
        android:layout_marginTop="@dimen/dimens_20"
        android:layout_marginEnd="@dimen/dimens_8"
        android:fontFamily="@font/ys_display_medium"
        android:text="Команда"
        android:textColor="@color/blackDayWhiteNight"
        android:textSize="@dimen/textSize_22"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_main"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dimens_16"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="@dimen/dimens_16"
        android:fontFamily="@font/ys_display_bold"
        android:text="Над приложением работали"
        android:textColor="@color/blackDayWhiteNight"
        android:textSize="@dimen/textSize_32"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_settings" />

    <TextView
        android:id="@+id/text_name1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/textSize_32"
        android:fontFamily="@font/ys_display_medium"
        android:text="Большаков Владимир - лид"
        android:textColor="@color/blackDayWhiteNight"
        android:textSize="@dimen/textSize_16"
        app:layout_constraintStart_toStartOf="@+id/text_main"
        app:layout_constraintTop_toBottomOf="@+id/text_main" />

    <TextView
        android:id="@+id/text_name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="Грачев Алексей"
        android:layout_marginTop="@dimen/dimens_16"
        android:textColor="@color/blackDayWhiteNight"
        android:textSize="@dimen/textSize_16"
        app:layout_constraintStart_toStartOf="@+id/text_main"
        app:layout_constraintTop_toBottomOf="@+id/text_name1" />

    <TextView
        android:id="@+id/text_name3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="Черепанов Владислав"
        android:layout_marginTop="@dimen/dimens_16"
        android:textColor="@color/blackDayWhiteNight"
        android:textSize="@dimen/textSize_16"
        app:layout_constraintStart_toStartOf="@+id/text_main"
        app:layout_constraintTop_toBottomOf="@+id/text_name2" />

    <TextView
        android:id="@+id/text_name4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="Усынин Максим"
        android:layout_marginTop="@dimen/dimens_16"
        android:textColor="@color/blackDayWhiteNight"
        android:textSize="@dimen/textSize_16"
        app:layout_constraintStart_toStartOf="@+id/text_main"
        app:layout_constraintTop_toBottomOf="@+id/text_name3" />

    <TextView
        android:id="@+id/text_name5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="Чернов Илья"
        android:layout_marginTop="@dimen/dimens_16"
        android:textColor="@color/blackDayWhiteNight"
        android:textSize="@dimen/textSize_16"
        app:layout_constraintStart_toStartOf="@+id/text_main"
        app:layout_constraintTop_toBottomOf="@+id/text_name4" />


</androidx.constraintlayout.widget.ConstraintLayout>